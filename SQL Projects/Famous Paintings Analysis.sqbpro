tics --

-- Famous Artists by Nationality --
SELECT a.nationality AS 'Nationality', COUNT(DISTINCT(a.artist_id)) AS 'Number of Artists', ROUND(COUNT(DISTINCT(a.artist_id)) * 100.0 / 
(SELECT COUNT(DISTINCT(a.artist_id)) FROM artist a), 2) AS '% of Total Artists'
FROM artist a
GROUP BY 1
ORDER BY 2 DESC;

-- Famous Artists by Art Style --
SELECT a.style AS 'Art Style', COUNT(DISTINCT(a.artist_id)) AS 'Number of Artists', ROUND(COUNT(DISTINCT(a.artist_id)) * 100.0 / 
(SELECT COUNT(DISTINCT(a.artist_id)) FROM artist a), 2) AS '% of Total Artists'
FROM artist a
GROUP BY 1
ORDER BY 2 DESC;

-- Famous Artists and Their Work --
SELECT a.full_name AS 'Artist Name', w.name AS 'Work Title'
FROM artist a
LEFT JOIN work w
	ON a.artist_id = w.artist_id
GROUP BY a.artist_id, 2
ORDER BY 1;

-- Famous Artists by Number of Paintings --
SELECT a.full_name AS 'Artist Name', COUNT(DISTINCT(w.work_id)) AS 'Number of Paintings'
FROM artist a
LEFT JOIN work w
	ON a.artist_id = w.artist_id
GROUP BY a.artist_id
ORDER BY 2 DESC;

-- Museums by Country --
SELECT m.country AS 'Country', COUNT(DISTINCT(m.museum_id)) AS ' Number of Museums'
FROM museum m
GROUP BY 1
ORDER BY 2 DESC;

-- Museums by City of Top Country --
SELECT m.city AS 'City in USA', COUNT(DISTINCT(m.museum_id)) AS ' Number of Museums'
FROM museum m
GROUP BY 1
HAVING m.country = 'USA'
ORDER BY 2 DESC;

-- Famous Paintings by Art Style --
SELECT w.style AS 'Art Style', COUNT(DISTINCT(w.work_id)) AS 'Number of Paintings'
FROM work w
GROUP BY 1
HAVING w.style IS NOT NULL
ORDER BY 2 DESC;

-- Famous Artists by Century --
SELECT ROUND((birth/100), 0) * 100 AS 'Century', COUNT(DISTINCT(artist_id)) AS 'Number of Artists'
FROM artist
GROUP BY 1
ORDER BY 2 DESC;
</sql><sql name="SQL 3">-- Analysis --

-- Which Museum Contains the Most Famous Artist Paintings? --
SELECT  m.country AS 'Country', m.city AS 'City', m.name AS 'Museum', COUNT(DISTINCT(w.work_id)) AS 'Number of Paintings'
FROM museum m
LEFT JOIN work w
	ON m.museum_id = w.museum_id
GROUP BY m.museum_id
ORDER BY 4 DESC
LIMIT 1;

-- What are the Top 10 Most Expensive Paintings? -- 
SELECT w.name AS 'Work Title', a.full_name AS 'Artist', p.sale_price AS 'Revenue'
FROM product_size p
LEFT JOIN work w
	ON p.work_id = w.work_id
LEFT JOIN artist a
	ON w.artist_id = a.artist_id
ORDER BY 3 DESC
LIMIT 10;

-- Which Museum Contains 'Fortuna' (The Most Expensive Painting) --
SELECT m.name AS 'Museum'
FROM museum m
LEFT JOIN work w
	ON m.museum_id = w.museum_id
WHERE w.name = 'Fortuna';
</sql><current_tab id="0"/></tab_sql></sqlb_project>
